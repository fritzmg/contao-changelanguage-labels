{
	"name":"fritzmg/contao-changelanguage-labels",
	"description":"Small Contao extensions for improved language labels in the site structure.",
	"keywords":["contao", "language", "labels", "changelanguage"],
	"type":"contao-module",
	"homepage":"http://www.inspiredminds.at",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Fritz Michael Gschwantner",
			"homepage":"http://www.inspiredminds.at",
			"email":"fmg@inspiredminds.at",
			"role":"Developer"
		}
	],
	"support":{
		"email":"fmg@inspiredminds.at",
		"issues":"https://github.com/fritzmg/contao-changelanguage-labels/issues",
		"source":"https://github.com/fritzmg/contao-changelanguage-labels",
		"forum":"https://community.contao.org/de"
	},
	"require":{
		"php":">=5.2",
		"contao/core":"~3.1",
		"contao-community-alliance/composer-installer":"*",
		"terminal42/contao-changelanguage": "*"
	},
	"replace": {
		"contao-legacy/changelanguage_labels": "self.version"
	},
	"extra":{
		"contao":{
			"sources": {
				"system/modules/changelanguage_labels":"system/modules/changelanguage_labels"
			}
		}
	}
}